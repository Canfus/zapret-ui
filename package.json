{
  "name": "zapret-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "build": {
    "appId": "com.canfus.zapret_ui"
  },
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc -b && vite build",
    "build:local": "pnpm build:vite && pnpm exec electron .",
    "build:exe": "electron-builder",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "format": "pnpm prettier && pnpm lint:fix"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^8.0.0-beta.13",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-tsconfig-paths": "^6.1.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "^8.0.0-beta.13"
    },
    "onlyBuiltDependencies": [
      "electron"
    ],
    "allowedBuildScripts": [
      "electron:*"
    ]
  }
}
